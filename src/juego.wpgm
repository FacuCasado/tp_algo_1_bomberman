import wollok.game.*
import bombita.*
import otros.*

program Bombita {

	//CONFIG
	game.title("Bombita")
	game.width(15)
	game.height(10)
	
	//VISUALES
	game.addVisualCharacter(new Jugador())
	8.times{i => game.addVisual(new Caja())} //Probablmente se puede mejorar para que spawneen mejor distribuidas las cajas
	
		
	//JUEGO
	game.start()
}

