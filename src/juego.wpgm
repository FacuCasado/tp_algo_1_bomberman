import wollok.game.*
import bombita.*
import otros.*

program Bombita {

	//CONFIG
	game.title("Bombita")
	game.width(26)
	game.height(12)
	
	//VISUALES
	game.addVisualCharacter(new Jugador())
	40.times{i => game.addVisual(new Caja())} //Probablmente se puede mejorar para que spawneen mejor distribuidas las cajas
	
		
	//JUEGO
	game.start()
}

